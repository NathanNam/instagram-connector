  <xs:element xmlns:mule="http://www.mulesoft.org/schema/mule/core" substitutionGroup="mule:abstract-extension" name="config-with-oauth">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="mule:abstractExtensionType">
          <xs:annotation>
            <xs:documentation></xs:documentation>
          </xs:annotation>
          <xs:sequence>
            <xs:element minOccurs="1" maxOccurs="1" name="oauth-callback-config">
              <xs:annotation>
                <xs:documentation>Config for http callbacks.</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:complexContent>
                  <xs:extension base="mule:abstractExtensionType">
                    <xs:attribute type="xs:string" use="required" name="localPort"></xs:attribute>
                    <xs:attribute type="xs:string" use="optional" name="remotePort"></xs:attribute>
                    <xs:attribute type="xs:string" use="required" name="domain"></xs:attribute>
                    <xs:attribute type="xs:boolean" use="optional" default="false" name="async"></xs:attribute>
                    <xs:attribute type="xs:string" use="required" name="path"></xs:attribute>
                    <xs:attribute type="xs:string" use="optional" name="connector-ref"></xs:attribute>
                    <xs:attribute type="xs:string" use="optional" name="defaultAccessTokenId">
                      <xs:annotation>
                        <xs:documentation>A Mule Expression to use as access token id</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                  </xs:extension>
                </xs:complexContent>
              </xs:complexType>
            </xs:element>
            <xs:element minOccurs="0" maxOccurs="1" name="oauth-store-config">
              <xs:annotation>
                <xs:documentation>Configuration element for storage of access tokens</xs:documentation>
              </xs:annotation>
              <xs:complexType>
                <xs:complexContent>
                  <xs:extension base="mule:abstractExtensionType">
                    <xs:attribute type="xs:string" use="required" name="objectStore-ref"></xs:attribute>
                  </xs:extension>
                </xs:complexContent>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
          <xs:attribute type="xs:string" use="required" name="name">
            <xs:annotation>
              <xs:documentation>Give a name to this configuration so it can be later referenced by config-ref.</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="required" name="consumerKey">
            <xs:annotation>
              <xs:documentation>Application's client identifier (consumer key)</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="required" name="consumerSecret">
            <xs:annotation>
              <xs:documentation>Application's client secret (consumer secret)</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="optional" default="basic" name="scope">
            <xs:annotation>
              <xs:documentation>Note that in order to use these extended permissions (comments+relationships+likes), first you need to submit your app for Instagram's review.
Extended permissions can be specified in scope. (i.e. scope=basic+comments+relationships+likes)</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="optional" default="https://api.instagram.com/oauth/access_token" name="accessTokenUrl">
            <xs:annotation>
              <xs:documentation>The URL defined by the Service Provider to obtain an access token</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/instagram" type="OnNoTokenPolicyEnumType" use="optional" default="EXCEPTION" name="onNoToken">
            <xs:annotation>
              <xs:documentation>The URL defined by the Service Provider to obtain an access token</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute type="xs:string" use="optional" default="https://api.instagram.com/oauth/authorize" name="authorizationUrl">
            <xs:annotation>
              <xs:documentation>The URL defined by the Service Provider where the resource owner will be redirected to grant authorization to the connector</xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/instagram" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="AuthorizeType" substitutionGroup="mule:abstract-message-processor" name="authorize">
    <xs:annotation>
      <xs:documentation>Starts OAuth authorization process. It must be called from a flow with an http:inbound-endpoint.</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/instagram" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="UnauthorizeType" substitutionGroup="mule:abstract-message-processor" name="unauthorize">
    <xs:annotation>
      <xs:documentation>Unauthorizes the connector, forcing to re-use authorize again before accessing any protected message processor.</xs:documentation>
    </xs:annotation>
  </xs:element>
